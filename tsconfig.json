{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["es2015"],
    "allowJs": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "es2015",
    "outDir": "dist",
    "moduleResolution": "node",
    "isolatedModules": true,
    "declaration": true
  },
  "include": ["./", "dist"],
  "exclude": []
}
